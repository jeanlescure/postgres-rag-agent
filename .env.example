# PostgreSQL Configuration
POSTGRES_USER=pglocaldev
POSTGRES_PASSWORD=dev-password-change-me
POSTGRES_DB=openclaw_rag

# Embedding Models
# Option 1: OpenAI (requires API key)
OPENAI_API_KEY=sk-...

# Option 2: Local Ollama (default)
OLLAMA_HOST=http://ollama:11434
OLLAMA_MODEL=nomic-embed-text

# Vector Configuration
VECTOR_DIMENSION=1536
EMBEDDING_MODEL=text-embedding-3-small
CHUNK_SIZE=512
CHUNK_OVERLAP=50

# PayloadCMS
PAYLOAD_SECRET=your-secret-key-here

# Elasticsearch
ELASTICSEARCH_HOST=elasticsearch:9200

# Apache Tika
TIKA_HOST=tika
TIKA_PORT=9998

# Application
NODE_ENV=development
LOG_LEVEL=info

# Database URL (constructed from above)
DATABASE_URL=postgresql://pglocaldev:dev-password-change-me@postgres:5432/openclaw_rag
